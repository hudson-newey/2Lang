{
    "name": "2lang",
    "displayName": "2Lang",
    "description": "2Lang language support for Visual Studio Code",
    "version": "0.0.1",
    "icon": "./icons/2lang.png",
    "engines": {
        "vscode": "^1.92.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "2lang",
                "extensions": [
                    ".2",
                    ".2.o"
                ],
                "icon": {
                    "light": "./icons/2lang.svg",
                    "dark": "./icons/2lang.svg"
                },
                "configuration": "./language-configuration.json"
            },
            {
                "id": "2lang-intermediate",
                "extensions": [
                    ".2.bin"
                ]
            }
        ],
        "grammars": [
            {
                "language": "2lang",
                "scopeName": "source.2",
                "path": "./syntaxes/2lang.tmLanguage.json"
            },
            {
                "language": "2lang-intermediate",
                "scopeName": "source.2.bin",
                "path": "./syntaxes/2lang-intermediate.tmLanguage.json"
            }
        ]
    }
}
